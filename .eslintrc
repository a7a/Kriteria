{
    "env": {
        "browser": false
       ,"node": true
      }
   ,"rules": {
        "quotes": 0
        //"quotes": [n, "single"|"double"|"backtick"]
       ,"camelcase": 0
       //,"camelcase": [1,{"properties": "never"}]
       ,"no-underscore-dangle": 0
       ,"no-process-exit": 0
       ,"no-unused-expressions": 1
       ,"no-unused-vars": 1
       ,"space-infix-ops": [1, {"int32Hint": true}]
       ,"no-use-before-define": 0
       ,"no-mixed-requires": 0
       ,"no-extra-boolean-cast": 0
    }
}

